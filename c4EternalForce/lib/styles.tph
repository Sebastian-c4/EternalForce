	COPY_EXISTING c4efJj7.itm ~override/c4efJj7%level%.itm~
		INNER_PATCH_SAVE level ~%level%~ BEGIN
			WRITE_BYTE 0 THIS - 16
		END
		LPF ADD_ITEM_EQEFFECT INT_VAR opcode=177 target=1 timing=4 duration=level*6+6 parameter2=2 STR_VAR resource=c4efJj7Y END
END
